{
	"name": "sveltekit-cartas-a-bris",
	"version": "1.4.0+5",
	"private": true,
	"scripts": {
		"dev": "dotenv -e .env.dev -- vite dev",
		"build": "dotenv -e .env.prod -- vite build",
		"preview": "dotenv -e .env.prod -- vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"prisma:pull:dev": "dotenv -e .env.dev -- prisma db pull",
		"prisma:pull:prod": "dotenv -e .env.prod -- prisma db pull",
		"prisma:generate:dev": "dotenv -e .env.dev -- prisma generate",
		"prisma:generate:prod": "dotenv -e .env.prod -- prisma generate",
		"prisma:migrate:dev": "dotenv -e .env.dev -- prisma migrate dev",
		"prisma:migrate:prod": "dotenv -e .env.prod -- prisma migrate deploy",
		"prisma:reset:dev": "dotenv -e .env.dev -- prisma migrate reset",
		"prisma:reset:prod": "dotenv -e .env.prod -- prisma migrate reset"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/node": "^20.12.12",
		"autoprefixer": "^10.4.17",
		"postcss": "^8.4.35",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prisma": "^5.14.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.1",
		"ts-node": "^10.9.2",
		"tslib": "^2.4.1",
		"tsx": "^4.11.0",
		"typescript": "^5.4.5",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^5.14.0",
		"dotenv": "^16.4.5",
		"firebase": "^10.12.2",
		"npm": "^10.2.4"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	}
}
